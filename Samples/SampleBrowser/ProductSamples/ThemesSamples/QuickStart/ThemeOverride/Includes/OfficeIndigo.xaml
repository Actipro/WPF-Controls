<ResourceDictionary 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:themes="http://schemas.actiprosoftware.com/winfx/xaml/themes">
    
	<SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.ContainerBackgroundMidLowBrushKey}" Color="#d8daeb" />
	<SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.ContainerBorderMidLowBrushKey}" Color="#8a8da8" />
	<SolidColorBrush x:Key="{x:Static themes:AssetResourceKeys.ContainerForegroundMidLowNormalBrushKey}" Color="#34374e" />
	
	<Style x:Key="DialogButtonStyle" TargetType="Button" BasedOn="{StaticResource {x:Static themes:SharedResourceKeys.ButtonBaseStyleKey}}">
		<Setter Property="themes:ThemeProperties.CornerRadius" Value="4" />
		<Setter Property="FontSize" Value="16" />
		<Setter Property="Padding" Value="20,7" />
	</Style>
	
</ResourceDictionary>